 </typeAliases>
 
 <mappers>
   <mapper resource="com/example/customers/customersMapper.xml"/>
   <mapper resource="com/example/goods/goodsMapper.xml"/>
   <mapper resource="com/example/note/noteMapper.xml"/>
   <mapper resource="com/example/tradeGoods/tradeGoodsMapper.xml"/>
 </mappers>
 
</configuration>  